<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
      <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
    <appSettings>
      <add key="KofaxRepositoryPath" value="C:\ACXMLAID\"/>
      <add key="KofaxErrorsRepositoryPath" value="C:\ACXMLAID\ERRORS\"/>
      <add key="DossiersEnAttenteRepositoryPath" value="C:\Application\IntegrationGed\DossiersEnAttente\"/>
      <add key="DossiersEnErreurRepositoryPath" value="C:\Application\IntegrationGed\DossiersEnErreur\"/>
      <add key="DossiersTraitesRepositoryPath" value="C:\Application\IntegrationGed\DossiersTraitÃ©s\"/>

      <add key="XmlBatchAttrName" value="HorsAssurance_PDF_{Current Date}_{Current Time}"/>
      <add key="XmlBatchAttrPriority" value="5"/>
      <add key="XmlBatchAttrBatchClassName" value="HA-AV-HorsAgeasMail"/>
      <add key="XmlBatchAttrProcessed" value="false"/>
      <add key="XmlBatchAttrRelativeImageFilePath" value=""/>
      <add key="XmlDocumentAttrImportFileName" value="HorsAssurance_DocumentMail"/>

      <add key="DossiersEnErreurPrefix" value="ERR_"/>
    </appSettings>
    <log4net>
      <appender name="ConsoleAppender" type="log4net.Appender.ColoredConsoleAppender">
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%date %level [%thread] %logger{1} %username - %message%newline"/>
        </layout>
        <mapping>
          <level value="WARN"/>
          <foreColor value="Yellow, HighIntensity"/>
        </mapping>
        <mapping>
          <level value="ERROR"/>
          <foreColor value="Red, HighIntensity"/>
        </mapping>
      </appender>
      <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
        <file value="C:\logs\logs\log.log"/>
        <rollingStyle value="Date"/>
        <appendToFile value="true"/>
        <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
        <datePattern value="yyyyMMdd"/>
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%date %level [%thread] %logger{1} - %message%newline"/>
        </layout>
      </appender>
      <root>
        <level value="INFO"/>
        <appender-ref ref="ConsoleAppender"/>
        <appender-ref ref="RollingFile"/>
      </root>
    </log4net>
</configuration>
